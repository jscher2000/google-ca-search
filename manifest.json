{
	"name": "Google Canada (google.ca) Search",
	"description": "Add google.ca to your search engines (including search suggestions from google.ca). Not an official Google add-on.",
	"version": "0.1",
	"manifest_version": 2,
	"applications":{
		"gecko": {
			"id": "google-ca-search@jeffersonscher.com",
			"strict_min_version": "64.0"
		}
	},
	"developer": {
		"name": "Jefferson \"jscher2000\" Scher",
		"url": "https://github.com/jscher2000/"
	},
	"icons": {
		"32": "favicon_ca_24.png"
	},
	"web_accessible_resources": [
		"favicon_ca_24.png"
	],
  "chrome_settings_overrides": {
		"search_provider": {
			"name": "Google CA",
			"search_url": "https://www.google.ca/search?q={searchTerms}",
			"suggest_url": "https://www.google.ca/complete/search?client=firefox&q={searchTerms}"
		}
	}
}
